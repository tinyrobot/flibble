jingo.declare({
	require: [
		'util.Class',
		'com.tinyrobot.jsg.display.DisplayObject',
		'com.tinyrobot.jsg.display.Graphics'
	],
  name: 'com.tinyrobot.jsg.display.Sprite',
  as: function() {
    Sprite = DisplayObject.extend({
			init: function(){
				this._super();
				this.graphics = new Graphics();
			},
			_render: function(cx){
				//somehow figure out dimensions here?
				this.graphics.render(cx);
			}
		});
  }
});
