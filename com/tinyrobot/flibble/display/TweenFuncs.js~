jingo.declare({
  name: 'com.tinyrobot.jsg.display.TweenFuncs',
  as: function() {
    TweenFuncs = {
    	easeInQuad: function(t,b,c,d){
				t /= d;
				return c*t*t + b;
			},
			linearTween: function(t,b,c,d){
				return c*t/d + b;
			},
			easeOutQuad: function (t, b, c, d) {
				t /= d;
				return -c * t*(t-2) + b;
			},
			defaultEase: function(t,b,c,d){
				return -1 * (t /= d) * (t - 2);
			}
    }
	}
});
